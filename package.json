{
  "name": "alicia-portfolio-site",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://AleeWeb.github.io/alicia-portfolio-site",
  "dependencies": {
    "aos": "^2.3.4",
    "bootstrap": "^4.3.1",
    "hover.css": "^2.3.2",
    "jquery": "^3.3.1",
    "materialize-css": "^1.0.0-rc.2",
    "overwatch-side-nav": "^0.1.13",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-reveal": "^1.2.2",
    "react-scripts": "^2.1.5",
    "react-scrollchor": "^6.0.0",
    "reactstrap": "^7.1.0",
    "styled-components": "^4.1.3",
    "ts-node": "^8.0.2",
    "ts-pnp": "^1.0.0",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5"
  }
}
